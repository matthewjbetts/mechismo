      readyFunction: jmol_isReady,
      disableJ2SLoadMonitor: true,
      disableInitialConsole: true,
      allowJavaScript: true
    };

    $(document).ready(function() {
      $("#appdiv").html(Jmol.getAppletHtml("jmolApplet0", Info))
    });

  </script>
</head>
<body>
  <div id="appdiv"></div>
  <a href="javascript:Jmol.script(jmolApplet0, 'if(_spinning);spin off;else;spin on;endif')">spin</a>
  | <a href="javascript:Jmol.script(jmolApplet0,'console')">console</a>
  | labels: <a href="javascript:Jmol.script(jmolApplet0,'set labelfront on; font label 24')">big</a> or <a href="javascript:Jmol.script(jmolApplet0,'set labelfront off; font label 12')">small</a>
</body>
