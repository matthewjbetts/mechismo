      script: "zap; load ASYNC /static/data/pdb/ex/pdb3ex7.ent.gz; restrict model = 1; background white; colour labels black; background labels lightgrey; set showHydrogens false; wireframe off; spacefill off; colour [xEEEEEE]; backbone 50; select (hetero and !hoh); wireframe 50; spacefill 100; colour cpk; select nucleic; colour cpk; define A2 22-413:C; define B2 3-146:A; select A2; colour [x00FFFF]; select B2; colour [xFF00FF]; define cr 248:C, 145:A; select cr; wireframe off; background labels white; select cr and (alpha, sidechain); wireframe 50; select cr and sidechain; colour cpk; define res 248:C, 248:C, 145:A; centre res; zoomto 1 {res} 0; define A2s 248:C; select A2s; wireframe off; select A2s and (alpha, sidechain); wireframe 100; colour label red; select A2s and sidechain; colour cpk; select 145A and alpha; label P145; select 248C and alpha; label 'S412F'; select (248:C and !(atomname = 'N' or atomname='CA' or atomname='O' or atomname='C' or atomname='CB')); colour pink; select (248:C and (atomname='CG' or atomname='CG1' or atomname='SG' or atomname='OG' or atomname='OG1')); spacefill 200; colour pink; select; set spinY 10;",
